; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PhyChiSim"
run/main_scene="res://scenes/ui/navui.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

scene_manager="*res://scripts/managers/SceneManager.gd"
transition_layer="*res://scenes/ui/transition_layer.tscn"
object_spawner="*res://scripts/managers/object_spawner.gd"
object_selector="*res://scripts/managers/object_selector.gd"
cursor_manager="*res://scripts/managers/cursor_manager.gd"
global_sim_variables="*res://scripts/simulation/global_sim_variables.gd"

[display]

window/stretch/mode="canvas_items"
window/dpi/allow_hidpi=false

[editor_plugins]

enabled=PackedStringArray("res://addons/bezier-line-2d/plugin.cfg")

